<%- await include('templates/header.ejs') %>

<div class="container">
	<div class="row">
		<h2> Programas </h2>
		<table class="table table-striped">
			<thead>
				<tr>
					<th> id </th>
					<th> nombre </th>
					<th> sede </th>
				</tr>
			</thead>
			<tbody>
				<% for (const post of programas) {	%>
				<tr>
					<td><%= post.id %></td>
					<td><%= post.nombre %></td>
					<td><%= post.sede %></td>
				</tr>
				<% } %>
			</tbody>
		</table>

	</div>
</div>
<%- await include('templates/footer.ejs') %>
